{
  "mcpServers": {
    "typescript-sdk": {
      "command": "node",
      "args": [
        "path/to/ts-sdk-server.js"
      ],
      "env": {}
    },
    "github": {
      "command": "node",
      "args": [
        "path/to/server-github"
      ],
      "env": {
        "GITHUB_TOKEN": "..."
      }
    },
    "puppeteer": {
      "command": "node",
      "args": [
        "path/to/server-puppeteer"
      ],
      "env": {}
    },
    "slack": {
      "command": "node",
      "args": [
        "path/to/server-slack"
      ],
      "env": {
        "SLACK_TOKEN": "..."
      }
    },
    "filesystem": {
      "command": "node",
      "args": [
        "path/to/server-filesystem"
      ],
      "env": {}
    },
    "memory-bank": {
      "command": "server-memory",
      "args": [],
      "env": {}
    },
    "sequential-thinking": {
      "command": "code-reasoning",
      "args": [],
      "env": {}
    },
    "brave-search": {
      "command": "server-brave-search",
      "args": [],
      "env": {}
    },
    "google-maps": {
      "command": "server-google-maps",
      "args": [],
      "env": {
        "GOOGLE_MAPS_API_KEY": "..."
      }
    },
    "deep-graph": {
      "command": "mcp-code-graph",
      "args": [],
      "env": {}
    },
    "supabase": {
      "type": "http",
      "url": "https://mcp.supabase.com/mcp"
    }
  }
}