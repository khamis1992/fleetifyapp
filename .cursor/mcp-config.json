{
  "mcpServers": {
    "playwright": {
      "command": "npx",
      "args": [
        "@playwright/mcp@latest"
      ],
      "env": {}
    },
    "security-scanner": {
      "command": "node",
      "args": [
        "scripts/security-scanner.js",
        "--scan-all",
        "--output",
        "json"
      ],
      "env": {}
    }
  }
}

